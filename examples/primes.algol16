"program primes
  local count, i, t, y, end_inner
begin
  read count;
  i := 2;
  while i <> count do
    end_inner := 0;
    if i mod 2 <> 0 or i = 2 then
      t := i;
      y := 3;
      while end_inner = 0 do
        if (y * y) > t then
          write t;
          end_inner := 1
        fi;
        if i mod y = 0 then
          end_inner := 1
        fi;
        y := y + 2
      done
    fi;
    i := i + 1
  done
end".
